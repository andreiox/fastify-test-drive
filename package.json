{
    "name": "fastify-test-drive",
    "version": "1.0.0",
    "description": "Trying fastify out",
    "main": "dist/server.js",
    "scripts": {
        "test": "ava",
        "start": "node dist/server.js",
        "build": "tsc",
        "dev": "nodemon --watch '*.ts' --exec sucrase-node src/server.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/andreiox/fastify-test-drive.git"
    },
    "keywords": [
        "fastify",
        "typescript"
    ],
    "author": "Andre Macedo",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/andreiox/fastify-test-drive/issues"
    },
    "homepage": "https://github.com/andreiox/fastify-test-drive#readme",
    "devDependencies": {
        "@types/node": "^14.0.27",
        "ava": "^3.10.1",
        "nodemon": "^2.0.4",
        "sucrase": "^3.15.0"
    },
    "dependencies": {
        "ajv": "^6.12.3",
        "dotenv": "^8.2.0",
        "fast-json-stringify": "^2.2.3",
        "fastify": "^3.29.4",
        "fastify-swagger": "^3.1.2",
        "pino-pretty": "^4.1.0",
        "typescript": "^3.9.7"
    },
    "ava": {
        "cache": false,
        "files": [
            "!dist"
        ],
        "extensions": [
            "ts"
        ],
        "require": [
            "sucrase/register/ts"
        ]
    }
}

{
	"name": "fastify-test-drive",
	"version": "1.0.0",
	"description": "Trying fastify out",
	"main": "index.js",
	"scripts": {
		"test": "ava",
		"start": "node dist/server.js",
		"build": "tsc",
		"dev": "nodemon --watch '*.ts' --exec sucrase-node server.ts"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/andreiox/fastify-test-drive.git"
	},
	"keywords": [
		"fastify",
		"typescript"
	],
	"author": "Andre Macedo",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/andreiox/fastify-test-drive/issues"
	},
	"homepage": "https://github.com/andreiox/fastify-test-drive#readme",
	"devDependencies": {
		"ava": "^3.10.1",
		"nodemon": "^2.0.4",
		"pino-pretty": "^4.1.0",
		"sucrase": "^3.15.0"
	},
	"dependencies": {
		"fastify": "^3.1.1",
		"typescript": "^3.9.7"
	},
	"ava": {
		"cache": false,
		"files": [
			"!dist"
		],
		"extensions": [
			"ts"
		],
		"require": [
			"sucrase/register/ts"
		]
	}
}
